{
  "expo": {
    "name": "Medicinal Plant ID",
    "slug": "medicinal-plant-identify",
    "version": "0.1.0",
    "scheme": "medplant",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to upload plant images for identification.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take plant photos for identification."
        }
      ],
      "expo-asset",
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "eas": {
