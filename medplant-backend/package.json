    "@google/genai": "^1.34.0",
    "@supabase/supabase-js": "^2.95.3",
    "@types/node": "^25.1.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "pdfkit": "^0.17.2",
    "razorpay": "^2.9.6",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/pdfkit": "^0.17.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
